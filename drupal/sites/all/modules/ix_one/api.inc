function getUpdated($filters, $display, $pageNumber = 1, $pageSize = 500)
{
	$base_url 	= 'https://services.ix-one.net/services/products/';

	$call		= 'updated/';
	$parameters	= 'StartDate=01/01/2014';
	$url 		= $base_url . $call . '?' . $parameters;
	echo($url);
	foreach($filters as $filter)		
	{
		$datafilters[] = addDataFilter($filter['value'], "Product", $filter['property'], $filter['logic']);
	}

	foreach($display as $property)
	{
		$propertylisting[] = addPropertyListings($property);
	}
	$data =
		array(
			"PageNumber" 		=> $pageNumber,
			"PageSize" 			=> $pageSize,
			"DataFilters" 		=> $datafilters,
			"PropertyListing" 	=> $propertylisting
		);
	$result = callAPI($data, $url);
	return $result;
}

function getIxOneImageListing()
{
	$count = 0;
	$ch = curl_init("ftp://unfiftp:j3k5qalM@ftp.logix3.com/");
	curl_setopt($ch, CURLOPT_FTPLISTONLY, true);
	curl_setopt($ch, CURLOPT_RETURNTRANSFER, true);
	curl_setopt($ch, CURLOPT_CONNECTTIMEOUT, 20);
	$start = microtime(true);
	$list = curl_exec($ch);
	$end = microtime(true) - $start;
	if(curl_errno($ch) != 0)
	{
		echo("FTP Error. Curl says: (".curl_errno($ch). ") ". curl_error($ch)."\n");
		return array();
	}

	$list = explode("\n",$list);
	foreach($list as $image)
	{
		if(preg_match('/([0-9]{13,14})_.*(tif|TIF|jpg|JPG)/',$image,$matches))
		{
			$items[] = $matches[1];
			$images[$matches[1]][] = $image;
			$count++;
		} 
	}
	return array_unique($items);
}

function callAPI($data, $url)
{
	$data_string = json_encode($data);

	$headers[]	= 'Content-Type: application/json';
	$headers[]	= 'Content-Length: ' . strlen($data_string);
	$headers[]	= 'Authorization: ' . TOKEN;

	$ch = curl_init($url);
	curl_setopt($ch, CURLOPT_CUSTOMREQUEST, "POST");
	curl_setopt($ch, CURLOPT_POSTFIELDS, $data_string);
	curl_setopt($ch, CURLOPT_RETURNTRANSFER, true);
	curl_setopt($ch, CURLOPT_HTTPHEADER, $headers);
	curl_setopt($ch, CURLOPT_SSL_VERIFYPEER, false);

	$http_request_time = microtime(true);

		$json_result = curl_exec($ch);

	$http_request_time = microtime(true) - $http_request_time;

	$result = json_decode($json_result);
	$result->http_request_time = $http_request_time;
	return $result;
}

function addDataFilter($comparitor, $entityName = "Product", $propertyName = "UPC12", $operator ="!=")
{
	return(
		array(
			"EntityName" 		=> $entityName,
			"PropertyName" 		=> $propertyName,
			"Operator" 			=> $operator,
			"Comparitor" 		=> $comparitor
		)
	);
}

function addPropertyListings($propertyName, $entityName = "Product")
{
	return(
		array(
			"EntityName" 		=> $entityName,
			"PropertyName" 		=> $propertyName
		)
	);
}
